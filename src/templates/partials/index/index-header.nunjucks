            {  id: 'slider11', url: '11.jpg' },
            {  id: 'slider12', url: '12.jpg' },
            {  id: 'slider13', url: '13.jpg' },
            {  id: 'slider14', url: '14.jpg' },
            {  id: 'slider15', url: '15.jpg' },
            {  id: 'slider16', url: '16.jpg' },
            {  id: 'slider17', url: '17.jpg' },
            {  id: 'slider18', url: '18.jpg' },
            {  id: 'slider19', url: '19.jpg' }
