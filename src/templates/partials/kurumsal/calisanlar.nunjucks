
<div class="row">
  {% for calisan in calisanlar %}
  <div class="col-6 col-sm-6 col-md-6 col-lg-3 my-2">
    <img class="calisan-img" src="img/kurumsal-yeni/{{calisan.image}}" />
    <p class="kisi-ad">{{calisan.isim}}</p>
    <p class="gorev">{{calisan.gorev}}</p>
    <p class="dahili-text">DAHİLİ: {{calisan.dahili}}</p>
  </div>
  {% endfor %}
</div>

